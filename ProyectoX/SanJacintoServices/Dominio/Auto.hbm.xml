<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SanJacintoServices"
                   namespace="SanJacintoServices.Dominio"
                   default-lazy="false">
  <class name="Auto" table="tb_auto">
    <id name="Codigo" column="codigo">
      <generator class="increment" />
    </id>
    <many-to-one name="Marca" column="marca" />
    <many-to-one name="Modelo" column="modelo" />
    <property name="Precio" column="precio" />
    <property name="Categoria" column="categoria" />
    <many-to-one name="Estado" column="estado" />
    <property name="Placa" column="placa" />
    <property name="Imagen" column="imagen" />
  </class>
</hibernate-mapping>