<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SanJacintoServices"
                   namespace="SanJacintoServices.Dominio"
                   default-lazy="false">
  <class name="Alquiler" table="tb_alquiler">
    <id name="Codigo" column="codigo">
      <generator class="increment" />
    </id>
    <property name="Costo" column="costo" />
    <property name="CostoAdicional" column="costo_adicional" />
    <property name="Igv" column="igv" />
    <property name="MontoTotal" column="monto_total" />
    <many-to-one name="Auto" column="codigo_auto" />
    <many-to-one name="Usuario" column="codigo_usuario" />
    <property name="FechaInicio" column="fecha_inicio" />
    <property name="FechaFin" column="fecha_fin" />
    <property name="CantidadDias" column="cant_dias" />
    <property name="Accesorios" column="accesorios" />
  </class>
</hibernate-mapping>